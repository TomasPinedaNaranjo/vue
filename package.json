{
  "name": "vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/ --ext .vue,.js --fix",
    "lint:check": "eslint src/ --ext .vue,.js",
    "format": "prettier --write \"src/**/*.{vue,js,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,json,css}\"",
    "lint:fix": "npm run lint && npm run format",
    "quality": "npm run lint:check && npm run format:check && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.5",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.10",
    "vite": "^6.3.5"
  }
}
